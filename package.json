{
  "name": "portfolioastro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "clean": "rimraf dist",
    "preview": "astro preview",
    "astro": "astro",
    "create-nojekyll": "node -e \"require('fs').writeFileSync('dist/.nojekyll', '')\"",
    "deploy": "npm run clean && astro build && npm run create-nojekyll && gh-pages -d dist --dotfiles"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.5",
    "astro": "^5.7.13"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "rimraf": "^6.0.1"
  }
}